generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model gps_data {
  id         Int       @id @default(autoincrement())
  latitude   Float?
  longitude  Float?
  speed      Float?
  altitude   Float?
  hdop       Float?
  satellites Int?
  timestamp  DateTime? @db.Timestamp(6)
  is_rem     Int?
}
